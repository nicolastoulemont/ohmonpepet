#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.3.4/node_modules/ts-node-dev/lib/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.3.4/node_modules/ts-node-dev/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.3.4/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules:/Users/nicolastoulemont/Code/node_modules:/Users/nicolastoulemont/node_modules:/Users/node_modules:/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/api/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.3.4/node_modules/ts-node-dev/lib/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.3.4/node_modules/ts-node-dev/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.3.4/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules:/Users/nicolastoulemont/Code/node_modules:/Users/nicolastoulemont/node_modules:/Users/node_modules:/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/api/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.3.4/node_modules/ts-node-dev/lib/bin.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.3.4/node_modules/ts-node-dev/lib/bin.js" "$@"
fi
