#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/gen-env-types@1.3.0/node_modules/gen-env-types/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/gen-env-types@1.3.0/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules:/Users/nicolastoulemont/Code/node_modules:/Users/nicolastoulemont/node_modules:/Users/node_modules:/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/api/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/gen-env-types@1.3.0/node_modules/gen-env-types/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/gen-env-types@1.3.0/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules:/Users/nicolastoulemont/Code/node_modules:/Users/nicolastoulemont/node_modules:/Users/node_modules:/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/api/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/gen-env-types@1.3.0/node_modules/gen-env-types/gen-env-types.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/gen-env-types@1.3.0/node_modules/gen-env-types/gen-env-types.js" "$@"
fi
