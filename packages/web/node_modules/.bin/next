#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/next@12.0.0_react-dom@17.0.1+react@17.0.1/node_modules/next/dist/bin/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/next@12.0.0_react-dom@17.0.1+react@17.0.1/node_modules/next/dist/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/next@12.0.0_react-dom@17.0.1+react@17.0.1/node_modules/next/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/next@12.0.0_react-dom@17.0.1+react@17.0.1/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules:/Users/nicolastoulemont/Code/node_modules:/Users/nicolastoulemont/node_modules:/Users/node_modules:/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/web/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/next@12.0.0_react-dom@17.0.1+react@17.0.1/node_modules/next/dist/bin/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/next@12.0.0_react-dom@17.0.1+react@17.0.1/node_modules/next/dist/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/next@12.0.0_react-dom@17.0.1+react@17.0.1/node_modules/next/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/next@12.0.0_react-dom@17.0.1+react@17.0.1/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules:/Users/nicolastoulemont/Code/node_modules:/Users/nicolastoulemont/node_modules:/Users/node_modules:/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/web/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/next@12.0.0_react-dom@17.0.1+react@17.0.1/node_modules/next/dist/bin/next" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/next@12.0.0_react-dom@17.0.1+react@17.0.1/node_modules/next/dist/bin/next" "$@"
fi
