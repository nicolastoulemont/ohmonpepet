#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/typescript@4.3.4/node_modules/typescript/bin/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/typescript@4.3.4/node_modules/typescript/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/typescript@4.3.4/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules:/Users/nicolastoulemont/Code/node_modules:/Users/nicolastoulemont/node_modules:/Users/node_modules:/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/web/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/typescript@4.3.4/node_modules/typescript/bin/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/typescript@4.3.4/node_modules/typescript/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/typescript@4.3.4/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules/.pnpm/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/node_modules:/Users/nicolastoulemont/Code/node_modules:/Users/nicolastoulemont/node_modules:/Users/node_modules:/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/web/node_modules:/Users/nicolastoulemont/Code/ohmonpepet/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/typescript@4.3.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/typescript@4.3.4/node_modules/typescript/bin/tsserver" "$@"
fi
